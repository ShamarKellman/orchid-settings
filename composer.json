{
    "name": "shamarkellman/orchid-settings",
    "description": "This is my package Settings",
    "keywords": [
        "ShamarKellman",
        "laravel",
        "orchid-settings"
    ],
    "homepage": "https://github.com/shamarkellman/orchid-settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Shamar Kellman",
            "email": "shamarkellman@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37",
        "orchid/platform":"^10.0.0",
        "ext-json": "*"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "ShamarKellman\\Settings\\": "src",
            "ShamarKellman\\Settings\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ShamarKellman\\Settings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShamarKellman\\Settings\\SettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
